{
  "name": "sky1-backend",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel start playground",
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x node --inspect -r ts-node/register src/index.ts",
    "playground": "graphql playground",
    "build": "rm -rf dist && graphql codegen && tsc",
    "prisma": "prisma"
  },
  "dependencies": {
    "@polka/send-type": "^0.4.0",
    "apollo-server": "^2.0.0",
    "apollo-server-core": "^2.0.0",
    "apollo-upload-server": "^6.0.0-alpha.1",
    "axios": "^0.18.0",
    "backblaze-b2": "^1.0.4",
    "cloudinary": "^1.11.0",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-middleware": "^1.5.0",
    "graphql-shield": "^2.2.6",
    "jsonwebtoken": "^8.3.0",
    "polka": "^0.4.0",
    "trouter": "^1.1.0"
  },
  "devDependencies": {
    "dotenv-cli": "^1.4.0",
    "dotenv-safe": "^6.0.0",
    "graphql-cli": "^2.16.5",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "prisma": "^1.12.0",
    "prisma-binding": "^2.1.1",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true
  }
}
