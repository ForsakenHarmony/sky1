{
  "name": "sky1-backend",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel start playground",
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x node --inspect -r ts-node/register src/index.ts",
    "playground": "graphql playground",
    "build": "rm -rf dist && graphql codegen && tsc",
    "prisma": "prisma"
  },
  "dependencies": {
    "@polka/send-type": "^0.5.0",
    "apollo-server": "^2.1.0",
    "apollo-server-core": "^2.1.0",
    "apollo-upload-server": "^7.1.0",
    "axios": "^0.18.0",
    "backblaze-b2": "^1.0.4",
    "cloudinary": "^1.11.0",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "graphql-middleware": "^1.7.7",
    "graphql-shield": "^3.2.5",
    "jsonwebtoken": "^8.3.0",
    "polka": "^0.5.1",
    "trouter": "^2.0.1"
  },
  "devDependencies": {
    "dotenv-cli": "^1.4.0",
    "dotenv-safe": "^6.1.0",
    "graphql-cli": "^2.16.7",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "prisma": "^1.18.1",
    "prisma-binding": "^2.1.6",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true
  }
}
